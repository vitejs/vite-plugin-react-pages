  "license": "ISC",
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "vite-pages-theme-basic": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@vitejs/plugin-react-refresh": "^1.1.1",
    "serve": "^11.3.2",
    "gh-pages": "^3.1.0",
    "vite": "^2.0.0-beta.12",
    "vite-plugin-mdx": "workspace:*",
    "vite-plugin-react-pages": "workspace:*"
  }
